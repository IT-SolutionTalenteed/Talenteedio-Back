{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/ent": "^2.2.5",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/request": "^2.48.8",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "dev": "nodemon",
    "ts-node": "ts-node",
    "build": "rm -rf build/* && tsc && find src -name '*.graphql' -exec cp --parents {} build ';' -o -name '*.handlebars' -exec cp --parents {} build ';' && cp -r build/src/* build && rm -r build/src",
    "start": "yarn build && node build/index",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json,.md src",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "typeorm:table:drop": "typeorm-ts-node-esm schema:drop -d ./src/database",
    "typeorm:db:init": "yarn typeorm:table:drop && yarn typeorm:migration:run && yarn typeorm:db:seed",
    "typeorm:entity:create": "typeorm entity:create",
    "typeorm:migration:generate": "typeorm-ts-node-esm migration:generate -d src/database",
    "typeorm:migration:create": "typeorm-ts-node-esm migration:create",
    "typeorm:migration:run": "typeorm-ts-node-esm migration:run -d src/database/index.ts",
    "typeorm:migration:revert": "typeorm-ts-node-esm migration:revert -d src/database",
    "typeorm:db:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -r src/database -d index",
    "generate:consent:blank": "ts-node ./src/helpers/scripts/generateBlankConsent",
    "generate:consent:all": "ts-node ./src/helpers/scripts/generateConsentForTalents",
    "export:cv": "ts-node ./src/helpers/scripts/exportCVs"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.11",
    "@graphql-tools/load": "^7.8.6",
    "@graphql-tools/resolvers-composition": "^6.5.12",
    "@graphql-tools/schema": "^9.0.10",
    "@graphql-tools/stitch": "^8.7.27",
    "@graphql-yoga/node": "^3.9.1",
    "@sentry/node": "^7.72.0",
    "@sentry/profiling-node": "^1.2.1",
    "@types/html-to-pdfmake": "^2.4.1",
    "@types/jsdom": "^21.1.2",
    "@types/pdfmake": "^0.2.2",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.0.3",
    "ent": "^2.2.0",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.17.3",
    "fs-extra": "^11.1.0",
    "generate-password": "^1.7.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.9.1",
    "html-to-pdfmake": "^2.4.25",
    "https": "^1.0.0",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^3.0.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "nodemailer-express-handlebars": "^6.0.0",
    "pdfmake": "^0.2.7",
    "tslib": "^2.4.1",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.7.0",
    "typeorm-store": "^2.0.1"
  }
}
